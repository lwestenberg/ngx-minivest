{
  "name": "ngx-minivest",
  "version": "1.0.0",
  "description": "Angular utilities for Vest.js form validation with signals",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
